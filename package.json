{
  "name": "express-boilerplate",
  "version": "0.1.0",
  "description": "This repo contains template project for creating APIs in express.",
  "main": "server.js",
  "scripts": {
    "test": "mocha --recursive \"./api/**/*.test.js\"",
    "start": "node server.js",
    "lint": "./node_modules/.bin/eslint .",
    "lint:fix": "./node_modules/.bin/eslint . --fix",
    "generate-docs": "./node_modules/apidoc/bin/apidoc -i ./ -f \".*\\.route[s]?.js$\" -o docs/"
  },
  "keywords": [
    "template",
    "microservices"
  ],
  "author": "Vijay Krishnavanshi",
  "license": "MIT",
  "dependencies": {
    "apidoc": "^0.24.0",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.14",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "express-validation": "^3.0.5",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.3",
    "morgan": "^1.10.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-security": "^1.4.0",
    "husky": "4.2.5",
    "mocha": "^8.1.1",
    "nock": "^13.0.4",
    "prettier": "^2.0.5"
  }
}
